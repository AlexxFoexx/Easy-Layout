@import 'nib'
@import 'rupture'
@import './var.styl'
@import './mixins/all-mixins.styl'
@import './fonts'
flex-version = flex
support-for-ie = false
vendor-prefixes = official

/*Адаптив*/
rupture.scale = 0 576px 768px 992px 1200px
rupture.scale-names = 'xs' 'sm' 'md' 'lg' 'xl'
	
*
	padding 0
	margin 0
	box-sizing: border-box
	&::after
		padding 0
		margin 0
		box-sizing: border-box
	&::before
		padding 0
		margin 0
		box-sizing: border-box

html
	font-size 16px
	+below(sm)
		font-size 15px

body
	line-height 1.5
	font-family default-text
	font-weight: 400
	color: def-color
	text-align: left
	background-color: #f9f9f9

article, aside, figcaption, figure, 
footer, header, hgroup, main, nav, section 
	display block

ol,li 
	list-style none

h1, h2, h3, h4, h5, h6
	margin 0
	font-family default-text

a
	text-decoration none
	background-color: transparent;
	color def-color
	outline: none
	&:hover
		text-decoration: none
		color: inherit

.no-offset
	padding 0
	margin 0

.nopadding
	padding 0

.nomargin
	margin 0

.s-pad
	padding 50px 0px
	+below(sm)
		padding: 30px 0px

.s-pad-bottom
	padding-bottom 50px
	+below(sm)
		padding-bottom: 30px

.s-pad-top
	padding-top 50px
	+below(sm)
		padding-top: 30px

.s-bg
	background #F7F7F7

.s-bg-dark
	background #191919

.s-title
	margin-bottom 30px
	line-height 1
	font-family 'OpenSansExtraBold'
	font-size rem(36px)
	color: #000
	+below(sm)
		font-size rem(30px)
	&--top
		margin: 30px 0px
	&--noffset
		+below(sm)
			margin-bottom: 15px
	&-white
		margin-bottom 30px
		line-height 1
		font-family 'OpenSansExtraBold'
		font-size rem(36px)
		color: def-color
		+below(sm)
			font-size rem(30px)
.hidden
	display none

.h-100
	height: 100%


.breadcrumbs
	&__link
		display: inline-block
		padding-bottom: 3px
		font-family: font7
		font-size: rem(15px)
		color: #000
		border-bottom: 1px solid #000
		transition(border-bottom)
		&:hover
			border-bottom 1px solid transparent
			color: #000
		&--white
			color #fff
			border-bottom: 1px solid #fff
			&:hover
				border-bottom 1px solid transparent
				color: #fff
	&__separator
		margin: 0px 8px
		font-family: font7
		font-size: rem(15px)
		color: #000
		&--white
			color #fff
.header
	position relative
	width 100%

	.logo-wrap
		width 120px
		display block
		+below(sm)
			width: 90px
		.logo
			width 100%
			object-fit: cover
	.header-menu
		position absolute
		top 0
		left 0
		width: 100%
		padding 20px 0px
		z-index: 2
		&--nopos
			position: static
			background: #191919
		&::after
			position absolute
			content ''
			bottom 0
			left 0
			width 100%
			height 1px
			background linear-gradient(90deg, transparent 10%, 
											  rgba(255,255,255, .1) 30%, 
											  rgba(255,255,255, .2) 50%, 
											  rgba(255,255,255, .1) 70%, 
											  transparent 90%)
		.h-nav
			position relative
			display flex	
			justify-content space-between
			align-items center
			margin: 0px
			+below(md)
				display none
			&-item
				position relative
				display: flex
				align-items: center
				cursor pointer
				&:nth-child(4) span
					position absolute
					top -7px
					right -9px
					size 14px
					display flex
					justify-content center
					align-items center
					border-radius: 50%
					font-size rem(10px)
					background #0000cc
				&:nth-child(5):hover .showreel-icon
					opacity: 1
			&__link
				position relative
				font-family 'OpenSansSemiBold'
				margin 0px 20px
				+below(lg)
					font-size rem(13px)
				&:hover
					color: #fff
			.line
				position absolute
				bottom -21px
				height 2px
				background #0000cc
				transition(all, .45s)



		.showreel
			&-icon
				position relative
				left -20px
				size 40px
				fill def-color
				opacity: .4
				transition(opacity, .45s)
.header-content
	width: 100%
	height 93.3vh
	+below(xs)
		height: 100vh
	&--nofull
		height: 50vh
	.header-idx-slider
		size 100%
		display flex
		justify-content center
		.container-slide
			position absolute
			top: 80px
			width 100%

		.swiper-slide
			size 100%
			background-repeat: no-repeat
			background-size cover
			background-position: center
			.overlay-bg
				position absolute
				top 0
				left 0
				size 100%
				background  linear-gradient(130deg, rgba(0,0,0,1) 30%, rgba(0,0,0, .9) 60% ,rgba(0,0,0,  .9) 50%, rgba(0,0,0, .8) 100%)
			.container-content
				position relative
				top 80px
				width: 100%
				height: 81.5%
				+below(sm)
					top 40px
				.slide-content-item
					height: 100%
					margin-top 60px

					.full-height
						height 100%
					.slide-content
						size 100%
						display flex
						flex-direction: column
						justify-content space-between
						&-title
							font-family 'HundaiSansHeadBold'
							font-size rem(120px)
							font-weight 700
							line-height 1
							transition(all, .25s)
							transform translateY(-40px)
							opacity: 0
							+below(md)
								font-size rem(80px)
							+below(sm)
								font-size rem(70px)
							+below(xs)
								font-size: rem(60px)
							&-is-active
								opacity: 1
								transform translateY(0)
						&-des
							display flex
							justify-content space-between
							align-items center
							margin-bottom: 20px
							+below(sm)
								flex-direction: column
								align-items: flex-start
							&-text
								width 70%
								+below(420px)
									width 100%
								p
									margin 0
									font-size rem(16px)
							&-play
								+below(sm)
									margin-top: 10px
								&:hover .slide-content-des-play__btn::after
									opacity 0
								&:hover .slide-content-des-icon
									opacity: 1

								&__btn
									position relative
									margin-right 23px
									+below(sm)
										font-size: rem(18px)
									&::after
										position absolute
										content ''
										bottom -4px
										left 0
										width 100%
										height 1px
										background  linear-gradient(to bottom, rgba(255,255,255, .4) 100%)
										transition(opacity, .45s)
							&-icon
								size 50px
								fill def-color
								opacity: .4
								transition(opacity, .45s)
		.container-navigation
			position absolute
			bottom 15%
			width 100%
			height 40px
			display flex
			align-items center
			+below(sm)
				bottom 32%
			.swiper-pagination
				position relative
				width auto
				display flex
				&-bullet
					background def-color
					opacity: .4
					margin-right 30px
					transition(opacity, .35s)
					.loading-dost
						size 20px
						opacity 0
					&-active
						position relative
						transform: scale(1.5)
						opacity: 1
						transition(opacity, .35s)
						border 1px solid def-color
						.loading-dost
							position absolute
							top: -5.5px
							left -5.5px
							opacity 1
							size 20px
							margin-bottom 20px
							transition(opacity, .35s)
							circle
								stroke-dasharray 1000
								stroke-dashoffset 1000
								animation dash 134s linear alternate infinite

			.swiper-button-next
				position relative
				top auto
				size: 50px
				margin 0 0 0 20px
				background transparent
				&:hover .swiper-button-next__icon
					opacity: 1
				+below(xs)
					display: none
				&__icon
					size 50px
					fill def-color
					opacity .4
					transition(opacity, .45s)


@keyframes dash 
	from 
		stroke-dashoffset 1000
	to 
		stroke-dashoffset 0

.start-project-btn 
	padding 8px 30px
	border-radius: 20px
	border 1px dashed transparent
	background #0000cc
	transition()
	&:hover
		border 1px dashed #0000cc
		color: #fff
		background: transparent
	+below(lg)
		font-size rem(12px)

.header-links
	display block
	width 100%
	background #0A0B0B
	padding 15px 0px
	+below(md)
		display none
	.lang
		height 100%
		display flex
		align-items center
		&-btn
			opacity: .4
			^[0] .active
				opacity: 1
		&-sep
			opacity .4
			margin 0px 10px

.social
	size 100%
	display flex
	justify-content flex-end
	align-items center
	&-links
		display flex
		.social-item
			display block
			size 35px
			margin-right 15px
			&__img
				size 100%
	.contacts-details
		position relative
		font-size rem(16px)
		&:hover::after
			opacity: 0
		&::after
			position absolute
			content ''
			bottom -4px
			left 0
			width 100%
			height 1px
			background  linear-gradient(to bottom, rgba(255,255,255, .4) 100%)
			transition(opacity, .45s)
.phone
	size 100%
	display flex
	justify-content flex-end
	align-items center
	&-text
		margin-right 15px
		font-size rem(16px)
		opacity: .4
	&-number
		font-size rem(18px)


.our-speciality
	width 100%
	.main-content
		width 90%
		color: #000
		margin-bottom 50px
		+below(sm)
			width: 100%
			margin-bottom: 30px
		&-title
			margin-bottom 20px
			font-family font4
			font-size rem(23px)
		&-text
			font-family font4
			font-size rem(16px)
	.speciality-block
		position relative
		display flex
		flex-direction: column
		padding 25px
		margin-bottom 20px
		background def-color
		overflow hidden
		&:hover::before
			animation: wave 1.5s ease-in-out alternate infinite
			bottom 0
		&:hover
			.speciality-des .speciality-des__title
				color: def-color
			.speciality-des .speciality-des__text
				color #9999ff
		&:hover .speciality-img
			transform: translateY(20px)

		&::before
			position absolute
			bottom -100%
			left 0
			content ''
			width: calc(100% + 50%)
			height 55%
			background url(../img/wave.png)
			transition(all, .85s)
		.speciality-img
			size 50px
			margin-bottom 60px
			transition(transform, .35s)
			+below(sm)
				margin-bottom: 30px
			&__icon
				size 100%
		.speciality-des
			position relative
			color: #000
			&__title
				font-family default-text
				font-size rem(17px)
				transition(color, .65s)
				transition-delay: .10s
			&__text
				font-family font3
				font-size rem(13px)
				color: def-gray
				transition(color, .55s)
				transition-delay: .15s

@keyframes wave
	to
		left 0
	from
		left -86px

/*
	СЕКЦИЯ КОМАНДА
*/
.team
	.team-human
		width 90%
		display flex
		align-items center
		justify-content space-between
		+below(md)
			width 100%
			margin-bottom 30px
		+below(sm)
			flex-wrap: wrap
		&-item
			display flex
			flex-direction column
			justify-content center
			align-items center
			+below(sm)
				width 50%
				margin-bottom 30px
			.team-human-img
				size 100px
				margin-bottom 12px
				border-radius: 50%
				overflow hidden
				&__photo
					size 100%
					object-fit: cover
			.team-human-des
				position relative
				left -3px
				display flex
				flex-direction column
				justify-content center
				&__firstname,&__lastname
					font-family default-text
					font-size rem(16px)
					color: def-black
				&__position
					margin-top 5px
					font-family: font3
					font-size rem(14px)
					color: def-gray
	.team-more
		width 100%
		display flex
		flex-direction column
		justify-content space-between
		+below(md)
			width 100%
		&__content
			p
				font-family default-text
				font-size rem(18px)
				color: def-black
		&__btn--wrap
			+below(md)
				display flex
				justify-content center
				align-items center
				margin-top 30px
/*
	КНОПКА С СТРЕЛКОЙ
*/
.blue__btn
	position relative
	display inline-block
	padding 8px 100px 8px 24px
	border 1px solid #1C1CCB
	border-radius: 20px
	font-family default-text
	font-size rem(15px)
	color: def-black
	overflow hidden
	transition(all, .35s)
	&:hover
		padding 8px 24px 8px 24px
		background #0707c6
		color def-color
	&:hover .blue-icon
		fill def-color
		right -50px
	.blue-icon
		position absolute
		top 0
		right 20px
		width 40px
		height 40px
		fill #0707c6
		transition(all, .35s)

/*
	СЕКЦИЯ ПРОЕКТЫ
*/

.project-main-view
	width 100%
	height 760px
	display flex
	flex-direction column
	justify-content space-between
	padding 45px
	background-repeat no-repeat
	background-size cover
	&:hover &__name
		transform translateX(20px)
	&:hover &-des
		transform translateX(20px)
	+below(sm)
		height 460px
		margin-bottom 30px
		padding 20px
	&__name
		font-family font3
		font-weight 200
		font-size rem(22px)
		color: #f6eeee
		transition(all, .35s)
		transition-delay: .25s
	&-des
		transition(all, .35s)
		&__develop
			font-family default-text
			font-size rem(28px)
			color: #f6eeee
	&-small
		display flex
		flex-direction column
		&__date
			font-family default-text
			font-size rem(16px)
			color: rgba(255,255,255, .5)
		&__tags
			font-family default-text
			font-size rem(13px)
			color: rgba(255,255,255, .5)

.project-veiw-items
	size 100%
	display flex
	flex-direction column
	justify-content space-between
	+below(sm)
		margin-bottom 30px
	&--row
		flex-direction: row
		+below(md)
			flex-direction: column
.project-view
	width 100%
	height 365px
	display flex
	flex-direction column
	justify-content space-between
	padding 45px
	&:hover .project-view__name
		transform translateX(20px)
	&:hover .project-view-des
		transform translateX(20px)
	+below(sm)
		padding 20px

	//Модификаторы блока
	&--row
		width: 48%
		+below(md)
			width: 100%
		margin-bottom: 30px
	&--bg
		background-repeat no-repeat
		background-size cover
		background-position center
	&--border
		border 1px solid #DDDDDD
		border-radius: 5px
	&-color
		color: def-color !important
	// Конец модификаторов блока
	&--pad
		margin-top: 30px
	&__name
		font-family font3
		font-weight 200
		font-size rem(22px)
		color: #f6eeee
		transition(all, .35s)
		transition-delay: .25s
		&--color
			color def-black
	&-des
		transition(all, .35s)
		&--color
			color: def-black !important
		&__develop
			font-family default-text
			font-size rem(22px)
			color: #f6eeee
			&--color
				color def-black
	&-small
		display flex
		flex-direction column
		&__date
			font-family default-text
			font-size rem(16px)
			color: rgba(255,255,255, .5)
			&--color
				color #909090
		&__tags
			font-family default-text
			font-size rem(13px)
			color: rgba(255,255,255, .5)
			&--color
				color #909090

.our-experience
	display flex
	justify-content space-between
	align-items center
	width 88%
	+below(md)
		width 100%
		margin-bottom 30px
	+below(sm)
		flex-wrap: wrap
	&-item
		display flex
		flex-direction column
		+below(sm)
			width 100%
			margin-bottom 30px
		&__number
			line-height 1
			font-family default-text
			font-size rem(48px)
			color: def-black
		&__text
			font-family default-text
			font-size rem(22px)
			color: def-black
	&-content
		p
			font-family default-text
			font-size rem(16px)
			color: def-gray
	&-btn
		display flex
		justify-content center
		align-items center


/*
	СЕКЦИЯ КЛИЕТЫ
*/


.clients-items
	width 100%
	.clients-item
		width 100%
		height auto
		display block
		&__img
			width 60%
			height inherit 
			object-fit: cover
.col-clients
	margin 0px 0px 50px 0px
	&:nth-child(6n)
		width 16.666667%
		height auto
		margin 0px 0px 50px 0px
	&:nth-child(n+7)
		width 16.666667%
		height auto
		margin 0
		+below(md)
			margin 0px 0px 50px 0px
/*
	СЕКЦИЯ ОТЗЫВЫ
*/
.reviews
	position relative
	overflow hidden
	+below(890px)
		padding: 0px 15px
	&-slider
		position static
		margin-bottom 100px
	&-slide
		width 100%
		display flex
		padding 30px
		border: 1px solid rgba(153,153,153, .7)
		border-radius: 4px
		+below(sm)
			flex-direction: column
		^[0]-img-block
			width 80px
			margin-right 20px
			overflow hidden
			.reviews-img-wrap
				size 80px
				.reviews-img
					size 100%
					border-radius: 50%
					object-fit: cover
		^[0]-des
			width 80%
			margin-top 20px
			+below(sm)
				width: 100%
			.reviews-main-des
				display flex
				flex-direction: column
				&__title
					font-family default-text
					font-size rem(22px)
					margin-bottom 5px
				&__position
					font-family font3
					font-size rem(12.5px)
					color: def-gray
					margin-bottom 30px
			&__text
				font-family font4
				font-size rem(16px)
				color: def-color
	^[0]-pagination
		bottom 15px !important
		+below(890px)
			bottom 40px !important
		.swiper-pagination-bullet
			background: def-color
			opacity: .4
			margin-right: 30px
			transition(opacity, .35s)
			&-active
				position relative
				transform: scale(1.2)
				opacity: 1
				transition(opacity, .35s)
				border 1px solid def-color


/*
	ФУТЕР
*/

.footer
	width 100%
	background #0A0A0A
	&-mail
		display flex
		flex-direction column
		+below(sm)
			align-items center
			margin-bottom 30px
		.footer-email
			line-height 1
			font-size rem(26px)
			margin-bottom 8px
		.footer-text
			font-weight font3
			font-size rem(14px)
			color: #5c5c5c
	.address
		display flex
		+below(sm)
			justify-content center
			margin-bottom 20px
		&-img
			width 24px
			margin-right 14px
			&__icon
				size 24px
		.address-des
			&__title
				font-size rem(14px)
				margin-bottom 5px
			&__subtitle 
				font-size rem(12px)
				color: #5c5c5c
	.studio
		display flex
		flex-direction column
		margin-top 50px
		+below(sm)
			align-items center
			margin-top: 10px
		&-title
			margin-bottom 15px
			font-family default-text
			font-size rem(16px)
			color: rgba(255,255,255, .5)
		&-polytics
			display inline-block
			margin-bottom 5px
			line-height 1
			font-family font3
			font-size rem(14px)
			color #5c5c5c
			transition()
			&:hover
				color #fff
		&-personal-data
			display inline-block
			line-height 1
			font-family font3
			font-size rem(14px)
			color #5c5c5c
			transition()
			&:hover
				color #fff
	.footer-nav
		display flex
		justify-content space-between
		margin-top: 50px
		margin-left 40px
		+below(md)
			display none
		&__link
			padding 5px 0px
			font-size rem(14px)
			transition()
			&:hover
				transform translateY(-7px)
	


/*
  МОДАЛЬНЫЕ ФОРМЫ
*/

.modal-form
	max-width: 550px
	min-width: 350px
	width: 100%
	display flex
	flex-direction column
	justify-content center
	align-items center
	background #fff
	padding 40px
	.fancybox-close-small
		color: #000
	&-conf
		max-width: 750px
		min-width: 290px
		&-text
			color: #000
			text-align left
	&-processing
		max-width: 750px
		min-width: 290px
		&-text
			color: #000
			text-align left
	&-head
		width: 100%
		display: flex
		flex-direction: column
		justify-content: center
		align-items: center
		margin-bottom 20px
		&__title
			margin-bottom 10px
			font-family: font5
